<Window x:Class="MasterP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MasterP"
        mc:Ignorable="d"
        Title="Работа с партнерами" Height="450" Width="800"
        Icon="Resource/Мастер пол.ico"
        FontFamily="Segoe UI">
    <Grid>

        <Rectangle Fill="#F4E8D3" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"/>

        <Image Source="Resource/Мастер пол.png" Width="100" Height="100" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10"/>

        <Button Content="Назад" Width="80" Height="30" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="10" Background="White" Foreground="Black"/>

        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Список партнеров" Width="200" Height="50" Background="#67BA80" Foreground="White" FontSize="16" Margin="10" Click="Button_Click"/>
            <Button Content="Отчеты" Width="200" Height="50" Background="#67BA80" Foreground="White" FontSize="16"  Margin="10" Click="Button_Click_Two"/>
            <Button Content="История реализации" Width="200" Height="50" Background="#67BA80" Foreground="White" FontSize="16"  Margin="10" Click="Button_Click_Tree"/>
        </StackPanel>
    </Grid>
</Window>